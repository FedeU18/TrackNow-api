{
  "kind": "collectionType",
  "collectionName": "usuarios",
  "info": {
    "singularName": "usuario",
    "pluralName": "usuarios",
    "displayName": "Usuario"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "nombre": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true
    },
    "apellido": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true
    },
    "documento": {
      "type": "biginteger",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true,
      "unique": true
    },
    "telefono": {
      "type": "biginteger",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true
    },
    "email": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true
    },
    "rol": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::rol.rol",
      "inversedBy": "usuarios"
    },
    "estado_usuario": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::estado-usuario.estado-usuario",
      "inversedBy": "usuarios"
    },
    "pedidos": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::pedido.pedido",
      "mappedBy": "cliente"
    },
    "notificacions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::notificacion.notificacion",
      "mappedBy": "usuario"
    },
    "pedidosAsignados": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::pedido.pedido",
      "mappedBy": "repartidor"
    }
  }
}
